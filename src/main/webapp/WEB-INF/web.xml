<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Servlets -->
  <servlet>
    <servlet-name>teiidServlet</servlet-name>
    <servlet-class>org.teiid.tools.webquery.server.TeiidServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>teiidServlet</servlet-name>
    <url-pattern>/webquery/teiid</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>webquery.html</welcome-file>
  </welcome-file-list>

</web-app>
